{
    "name": "saxulum/saxulum-assetic-twig-provider",
    "description": "Saxulum Assetic Twig Provider",
    "keywords": ["saxulum", "pimple", "cilex", "silex", "assetic", "twig"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3,<8.0",
        "kriswallsmith/assetic": "~1.2",
        "pimple/pimple": ">=2.1,<4",
        "symfony/finder": "~2.3",
        "symfony/http-foundation": "~2.3",
        "twig/twig": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "cssmin/cssmin": "~3.0.1",
        "mrclay/minify": "~2.2.0",
        "oyejorge/less.php": "~1.7",
        "leafo/scssphp": "~0.1",
        "silex/api": "~2.0@dev",
        "silex/providers": "~2.0@dev"
    },
    "suggest": {
        "cssmin/cssmin": "~3.0.1",
        "mrclay/minify": "~2.2.0",
        "oyejorge/less.php": "~1.7",
        "leafo/scssphp": "~0.1",
        "saxulum/saxulum-console": "~2.0",
        "silex/api": "~2.0@dev",
        "silex/providers": "~2.0@dev"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "cssmin/cssmin",
                "version": "3.0.1",
                "dist": { "url": "http://cssmin.googlecode.com/files/cssmin-v3.0.1.php", "type": "file" },
                "autoload": { "classmap": [ "cssmin-v3.0.1.php" ] }
            }
        }
    ],
    "autoload": {
        "psr-0": { "Saxulum\\AsseticTwig": "src/" }
    }
}
